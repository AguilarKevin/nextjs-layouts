{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:relay/recommended"
  ],
  "plugins": ["relay"],
  "rules": {
    "no-unused-labels": "warn",
    "no-unused-vars": ["warn", {"args": "none", "ignoreRestSiblings": true}],
    "relay/generated-flow-types": "off"
  }
}
